#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin>>n;
    int a[n],i,ev=0,od=0;
    for(i=0;i<n;i++)
    {   cin>>a[i];
        if(a[i]&1)
            od++;
        else
            ev++;
        
    }
    if(od>=ev)   cout<<"NOT READY\n";
    else    cout<<"READY FOR BATTLE\n";
}
